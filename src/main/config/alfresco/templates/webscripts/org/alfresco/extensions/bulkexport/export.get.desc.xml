<webscript>
  <shortname>Bulk Export</shortname>
  <description>Export data as Bulk format</description>
  <url>/extensions/bulkexport/export?nodeRef={NodeRef}&amp;base={base}&amp;ignoreExported={ignoreExported?}&amp;exportVersions={exportVersions}</url>
  <authentication>admin</authentication>
  <transaction allow="readonly">required</transaction>
  <format default="html">extension</format>
  <cache>
    <never>true</never>
  </cache>
</webscript>
